class Vec2
{
    Vec2();
    Vec2(const Vec2& v);
    Vec2(float x, float y);
    ~Vec2();

    Vec2 operator+(const Vec2& v) const;
    Vec2 operator-(const Vec2& v) const;
    Vec2 operator*(float s) const;
    Vec2 operator/(float s) const;
    bool operator==(const Vec2& v);

    
    float& x() { return _x; }
    float& y() { return _y; }
    
    Vec2 normalize();
    float dot(const Vec2& v);
    float length();

};